/* Breath-first search*/

function bfs(graph, root) {
var nodesLen = {};

for (var i = 0; i < graph.length; i++) {
nodeSLen[i] = infinity}
nodesLen[root] = 0;

var queue = [root];
var current;

while (queue.length != 0) {
current = queue.shift()

var curConnected = graph[current];
var neigborIdx = [];
var idx = curConnected.indexOf(1);
while (idx != -1) {
neighborIdx.push(idx);

idx = curConnected.Index
}
};


}
